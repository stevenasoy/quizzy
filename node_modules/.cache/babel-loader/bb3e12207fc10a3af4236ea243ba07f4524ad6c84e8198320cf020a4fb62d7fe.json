\"\n};\nexport function render(_ctx, _cache, $props, $setup, $data, $options) {\n  return _openBlock(), _createElementBlock(\"div\", _hoisted_1, [_createElementVNode(\"div\", _hoisted_2, [_cache[0] || (_cache[0] = _createElementVNode(\"h2\", null, \"Study Progress\", -1 /* HOISTED */)), _createElementVNode(\"div\", _hoisted_3, [(_openBlock(), _createElementBlock(_Fragment, null, _renderList($setup.timeRanges, function (range) {\n    return _createElementVNode(\"button\", {\n      key: range,\n      \"class\": _normalizeClass(['range-btn', {\n        active: $setup.selectedRange === range\n      }]),\n      onClick: function onClick($event) {\n        return $setup.selectedRange = range;\n      }\n    }, _toDisplayString(range), 11 /* TEXT, CLASS, PROPS */, _hoisted_4);\n  }), 64 /* STABLE_FRAGMENT */))])]), _createElementVNode(\"div\", _hoisted_5, [_createElementVNode(\"div\", _hoisted_6, [_createElementVNode(\"div\", _hoisted_7, _toDisplayString($setup.totalReviews), 1 /* TEXT */), _cache[1] || (_cache[1] = _createElementVNode(\"div\", {\n    \"class\": \"stat-label\"\n  }, \"Total Reviews\", -1 /* HOISTED */))]), _createElementVNode(\"div\", _hoisted_8, [_createElementVNode(\"div\", _hoisted_9, _toDisplayString($setup.currentStreak), 1 /* TEXT */), _cache[2] || (_cache[2] = _createElementVNode(\"div\", {\n    \"class\": \"stat-label\"\n  }, \"Day Streak\", -1 /* HOISTED */))]), _createElementVNode(\"div\", _hoisted_10, [_createElementVNode(\"div\", _hoisted_11, _toDisplayString($setup.averageAccuracy) + \"%\", 1 /* TEXT */), _cache[3] || (_cache[3] = _createElementVNode(\"div\", {\n    \"class\": \"stat-label\"\n  }, \"Average Accuracy\", -1 /* HOISTED */))]), $setup.dueCount > 0 ? (_openBlock(), _createElementBlock(\"div\", {\n    key: 0,\n    \"class\": \"stat-card review-due-card\",\n    onClick: $setup.startReview\n  }, [_createElementVNode(\"div\", _hoisted_12, _toDisplayString($setup.dueCount), 1 /* TEXT */), _cache[4] || (_cache[4] = _createElementVNode(\"d