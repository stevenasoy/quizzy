        </div>\r\n        <div class=\"stat-card\">\r\n          <div class=\"stat-value\">{{ currentStreak }}</div>\r\n          <div class=\"stat-label\">Day Streak</div>\r\n        </div>\r\n      </div>\r\n      <button \r\n        @click=\"startSession\" \r\n        class=\"start-btn\"\r\n        :disabled=\"dueQuestions.length === 0\"\r\n      >\r\n        Start Review\r\n      </button>\r\n    </div>\r\n\r\n    <!-- Review Interface -->\r\n    <div v-else-if=\"!completed\" class=\"review-interface\">\r\n      <div class=\"progress-bar\">\r\n        <div \r\n          class=\"progress-fill\"\r\n          :style=\"{ width: `${(currentIndex + 1) / dueQuestions.length * 100}%` }\"\r\n        ></div>\r\n      </div>\r\n      \r\n      <div class=\"question-card\" :class=\"{ flipped: showAnswer }\">\r\n        <div class=\"card-front\" @click=\"showAnswer = true\">\r\n          <div class=\"question-text\">{{ currentQuestion.text }}</div>\r\n          <div class=\"click-hint\">Click to show answer</div>\r\n        </div>\r\n        \r\n        <div class=\"card-back\">\r\n          <div class=\"answer-text\">{{ currentQuestion.correctAnswer }}</div>\r\n          <div class=\"rating-buttons\">\r\n            <button \r\n              v-for=\"rating in ratings\" \r\n              :key=\"rating.value\"\r\n              @click=\"rateAnswer(rating.value)\"\r\n              class=\"rating-btn\"\r\n              :class=\"rating.class\"\r\n            >\r\n              {{ rating.label }}\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Session Complete -->\r\n    <div v-else class=\"session-complete\">\r\n      <h2>Review Complete!</h2>\r\n      <div class=\"completion-stats\">\r\n        <div class=\"stat-card\">\r\n          <div class=\"stat-value\">{{ stats.totalReviewed }}</div>\r\n          <div class=\"stat-label\">Cards Reviewed</div>\r\n        </div>\r\n        <div class=\"stat-card\">\r\n          <div class=\"stat-value\">{{ stats.accuracy }}%</div>\r\n          <div class=\"stat-label\">Accuracy</div>\r\n        </div>\r\n      </div>\r\n      <div class=\"next-due\">\r\n        <h3>Next Reviews</h3>\r\n        <div class=\"forecast-grid\">\r\n          <div \r\n            v-for=\"(count, date) in reviewForecast\" \r\n            :key=\"date\"\r\n            class=\"forecast-item\"\r\n          >\r\n            <div class=\"date\">{{ date }}</div>\r\n            <div class=\"count\">{{ count }} cards</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <button @click=\"$emit('close')\" class=\"close-btn\">Close</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, computed } from 'vue';\r\nimport { useStore } from 'vuex';\r\n\r\nconst store = useStore();\r\nconst emit = defineEmits(['close']);\r\n\r\n// State\r\nconst started = ref(false);\r\nconst completed = ref(false);\r\nconst showAnswer = ref(false);\r\nconst currentIndex = ref(0);\r\nconst sessionResponses = ref([]);\r\n\r\n// Computed\r\nconst dueQuestions = computed(() => store.getters['spacedRepetition/dueQuestions']);\r\nconst currentStreak = computed(() => store.getters['spacedRepetition/currentStreak']);\r\nconst currentQuestion = computed(() => dueQuestions.value[currentIndex.value]);\r\nconst reviewForecast = computed(() => store.getters['spacedRepetition/reviewForecast'](7));\r\n\r\nconst stats = computed(() => {\r\n  if (sessionResponses.value.length === 0) return { totalReviewed: 0, accuracy: 0 };\r\n  \r\n  const correct = sessionResponses.value.filter(r => r.performance >= 3).length;\r\n  return {\r\n    totalReviewed: sessionResponses.value.length,\r\n    accuracy: Math.round((correct / sessionResponses.value.length) * 100)\r\n  };\r\n});\r\n\r\n// Rating options\r\nconst ratings = [\r\n  { value: 0, label: 'Again', class: 'again' },\r\n  { value: 3, label: 'Hard', class: 'hard' },\r\n  { value: 4, label: 'Good', class: 'good' },\r\n  { value: 5, label: 'Easy', class: 'easy' }\r\n];\r\n\r\n// Methods\r\nconst startSession = () => {\r\n  started.value = true;\r\n  currentIndex.value = 0;\r\n  sessionResponses.value = [];\r\n};\r\n\r\nconst rateAnswer = async (performance) => {\r\n  const response = {\r\n    questionId: currentQuestion.value.id,\r\n    performance,\r\n    timestamp: new Date().toISOString()\r\n  };\r\n  \r\n  sessionResponses.value.push(response);\r\n  \r\n  // Update question in store\r\n  await store.dispatch('spacedRepetition/updateQuestionAfterReview', {\r\n    question: currentQuestion.value,\r\n    performance,\r\n    responseTime: null\r\n  });\r\n\r\n  // Move to next question or end session\r\n  if (currentIndex.value < dueQuestions.value.length - 1) {\r\n    currentIndex.value++;\r\n    showAnswer.value = false;\r\n  } else {\r\n    completed.value = true;\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.review-session {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 2rem;\r\n}\r\n\r\n.stats-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));\r\n  gap: 1rem;\r\n  margin: 2rem 0;\r\n}\r\n\r\n.stat-card {\r\n  background: #f5f5f5;\r\n  padding: 1.5rem;\r\n  border-radius: 8px;\r\n  text-align: center;\r\n}\r\n\r\n.stat-value {\r\n  font-size: 2rem;\r\n  font-weight: bold;\r\n  color: #2196F3;\r\n}\r\n\r\n.stat-label {\r\n  color: #666;\r\n  margin-top: 0.5rem;\r\n}\r\n\r\n.start-btn, .close-btn {\r\n  background: #2196F3;\r\n  color: white;\r\n  border: none;\r\n  padding: 1rem 2rem;\r\n  border-radius: 8px;\r\n  font-size: 1.1rem;\r\n  cursor: pointer;\r\n  transition: all 0.2s;\r\n}\r\n\r\n.start-btn:hover, .close-btn:hover {\r\n  background: #1976D2;\r\n  transform: translateY(-1px);\r\n}\r\n\r\n.start-btn:disabled {\r\n  background: #ccc;\r\n  cursor: not-allowed;\r\n  transform: none;\r\n}\r\n\r\n.progress-bar {\r\n  width: 100%;\r\n  height: 4px;\r\n  background: #e0e0e0;\r\n  margin-bottom: 2rem;\r\n  border-radius: 